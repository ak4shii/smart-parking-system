# ===========================================
# Smart Parking System - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values:
#   copy env.example .env   (Windows)
#   cp env.example .env     (Linux/Mac)
# ===========================================

# ----- MQTT Security -----
# Backend service password (CHANGE THIS IN PRODUCTION!)
MQTT_BACKEND_PASSWORD=your_secure_password_here

# Public MQTT broker address (for ESP32 devices to connect)
MQTT_PUBLIC_HOST=your.server.ip.or.domain
MQTT_PUBLIC_PORT=1883

# ----- Database -----
# PostgreSQL credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=admin
POSTGRES_DB=sps_db

# ----- Backend -----
# Spring Boot profile (dev/prod)
SPRING_PROFILES_ACTIVE=dev

# Log level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO

# ----- AWS S3 (Optional - for license plate images) -----
# S3_BUCKET_NAME=your-bucket-name
# S3_REGION=ap-southeast-1
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key

